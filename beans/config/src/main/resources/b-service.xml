<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- initializing CommonAnnotationBeanPostProcessor is same as context:annotation-config -->
    <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor"/>
    <bean name="someBeanXmlJava" class="com.olexyn.about.java.spring.config.beans.JavaHookBean">
        <property name="description" value="XML-Java"/>
    </bean>
</beans>
